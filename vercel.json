{
  "outputDirectory": "web",
  "buildCommand": "",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://nexypher-api.onrender.com/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ]
}
