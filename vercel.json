{
  "alias": ["pumpiq.com", "www.pumpiq.com"],
  "builds": [
    {
      "src": "web_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/web/static/(.*)",
      "dest": "/web/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web_app.py"
    }
  ]
}
